.screencast-header

  &_style_webpack
    background #67A2D2 url("screencast-header_style_webpack.jpg") 50% 50% no-repeat

  &_style_webpack &__title-accent
    text-transform uppercase

    color #FFFB9D

  &_style_webpack &__button .simple-button__text
    color #FFFB9D

  &_style_webpack &__info,
  &_style_webpack .share-icons__title,
  &_style_webpack .donate-links__title
    color rgba(255,255,255,0.7)

  &_style_webpack .share-icons .share
    color #FFFB9D

  &_style_webpack .share-icons .share:hover,
  &_style_webpack &__foot
    color #fff

  &_style_webpack:after
    background url("data:image/svg+xml;base64,")
